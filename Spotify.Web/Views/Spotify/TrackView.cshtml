@model SpotifyTrack 
@{
    ViewData["Title"] = "Track";
}

<div class="box">
    <div class="box-primary">
        <div class="box-header">
            <h3>@Model.Name - @Model.Artists.FirstOrDefault().Name</h3>
        </div>
        <div class="box-body">
        </div>
    </div>
</div>

<template id="playlist-builder-row">
    <div class="row">

    </div>
</template>


@section Scripts {
    <script>
        $(spotify.page.playlistBuilder.init)
    </script>
} 